.top {
  margin-top: px2vw(20);
}
.white {
  background: #fff;
}
.left {
  font-size: px2vw(30);
  font-weight: 400;
  color: #262626;
}
.content {
  height: px2vw(110);
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  .right {
    flex: 1;
    margin-left: px2vw(40);
    font-size: px2vw(24);
    font-weight: 300;
    color: #a8a8a8;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    .iconfont {
      font-size: px2vw(12);
      margin-top: px2vw(6);
    }
    .selectEnd {
      font-size: px2vw(30);
      font-weight: 400;
      color: #262626;
    }
  }
}
.bottom {
  display: flex;
  flex-direction: row;
  overflow: hidden;
  overflow-x: scroll;
  p {
    width: px2vw(130);
    height: px2vw(56);
    min-width: px2vw(130);
    max-width: px2vw(130);
    line-height: px2vw(56);
    text-align: center;
    box-sizing: border-box;
    border: px2vw(1) solid #7d7d7d;
    color: #7d7d7d;
    border-radius: px2vw(6);
    flex: 1;
    font-size: px2vw(24);
  }
  .active {
    border-color: #3e8fff;
    color: #3e8fff;
  }

  .item:not(:first-child) {
    margin-left: px2vw(30);
  }
}
.bottom::-webkit-scrollbar {
  display: none;
}
.product {
  font-size: 0;
  padding: px2vw(21) px2vw(40) px2vw(20);
  .name {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: px2vw(32);
    font-weight: 500;
    color: #262626;
    .iconfont {
      font-size: px2vw(8);
      color: #7d7d7d;
      font-weight: 400;
    }
  }
  .rate{
    height:px2cw(32);
    font-size:px2cw(32);
    font-family:PingFangSC-Medium,PingFangSC;
    font-weight:500;
    color:rgba(247,90,80,1);
    line-height:px2cw(32);
  }
  .num {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: px2vw(26);
    font-weight: 400;
    color: #a8a8a8;
    margin-top: px2vw(11);
  }
}
.payCard {
  background: #fff;
  padding: px2vw(20) px2vw(40);
  height: px2vw(110);
  display: flex;
  box-sizing: border-box;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  .right {
    margin-left: px2vw(40);
    flex: 1;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    .bank {
      display: flex;
      flex-direction: column;
      justify-content: space-between;
      p {
        font-size: 0;
        span {
          font-size: px2vw(30);
          color: #262626;
        }
      }
      .remain {
        font-size: 0;
        span {
          color: #7d7d7d;
          font-size: px2vw(24);
          font-weight: 300;
        }
      }
    }
  }
}
.money {
  background: #fff;
  height: px2vw(240);
  padding: 0 px2vw(40);
  .title {
    height: px2vw(80);
    line-height: px2vw(80);
    font-size: px2vw(30);
    font-weight: 400;
    color: #262626;
    margin-top: px2vw(20);
  }
  .toast {
    height: px2vw(60);
    line-height: px2vw(60);
    color: #7d7d7d;
    font-size: px2vw(24);
  }
}
.period {
  height: px2vw(110);
  background: #fff;
  padding: 0 px2vw(40);
}
.methods {
  height: px2vw(110);
  background: #fff;
  padding: 0 px2vw(40);
  .right {
    font-size: px2vw(30);
  }
}

.act {
  height: px2vw(196);
}
.referrer {
  height: px2vw(110);
  background: #fff;
  padding: 0 px2vw(40);
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  font-size: px2vw(30);
  .right {
    flex: 1;
    margin-left: px2vw(40);
  }

  //   background: linear-gradient(#ffdfc7, #d69f81);
}
/deep/ .van-field__control:disabled {
  color: #a8a8a8;
  font-size: px2vw(30);
}
/deep/ .van-field__control {
  color: #262626;
  font-size: px2vw(30);
  // margin-top: px2vw(-6);
}
/deep/ .md-field-item-content{
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
/deep/ .md-field-item-content:before {
  border-bottom: px2vw(1) solid #eff0f1;
}
/deep/ .md-amount {
  font-size: px2vw(24);
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(255, 181, 43, 1);
  margin-top: px2vw(18);
}
/deep/ .md-input-item {
  height: px2vw(66);
  line-height: px2vw(66);
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-bottom: px2vw(29);
}
/deep/ .md-input-item-input{
  font-size:px2vw(40);
}
.btn {
  font-size: px2vw(34);
  font-weight: 400;
  color: #ffffff;
  height: px2vw(90);
  line-height: px2vw(90);
  border-radius: px2vw(45);
  text-align: center;
  background:linear-gradient(180deg,rgba(255,190,78,1) 0%,rgba(255,159,8,1) 100%);
  margin: 0 px2vw(40);
  margin-top: px2vw(100);
}
.input-operator{
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:rgba(0,139,255,1);
}

.notice {
  margin-top: px2vw(40);
  font-size: px2vw(22);
  font-family: PingFangSC;
  font-weight: 400;
  color: #7D7D7D;
  line-height: px2vw(30);
  padding: 0 px2vw(40);
  p{
    margin-top: px2vw(6);
  }
  span{
    color:#008BFFFF
  }
}
.outer .teshu{
  height:px2vw(24);
  font-size:px2vw(24);
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:rgba(125,125,125,1);
  line-height:px2vw(24);
  margin-left: px2vw(40);
}
//下部选择转出方式
.rollout_means{
  display: flex;
  align-items: center;
  justify-content: flex-start;
  padding:  px2vw(20) px2vw(40)  px2vw(20) ;
  height: px2vw(220);
  background: #fff;
}
.radio_one {
  display: inline-block;
  height: px2vw(32);
  width: px2vw(32);
  line-height: px2vw(32);
  vertical-align: middle;
  cursor: pointer;
  background: url('~@/assets/travelTreasure/rollout/icon_weigouxuan_nor@2x.png') no-repeat;
  background-size: 100% 100%;
}
.input_radio {
  display: inline-block;
  height: px2vw(32);
  width: px2vw(32);
  line-height: px2vw(32);
  cursor: pointer;
  left: 0px;
  outline: none;
  -webkit-appearance: none;
}
.on {
  background: url('~@/assets/travelTreasure/rollout/icon_gouxuan_nor@2x.png') no-repeat;
  background-size: 100% 100%;
}
.rollout_{
  height: px2vw(180);
  display: flex;
  flex-direction: column;
 justify-content: space-between;
}
.rollout_left{
  height: px2vw(140);
  display: flex;
  flex-direction: column;
 justify-content: space-between;
}
.rollout_right{
  height: px2vw(180);
  display: flex;
  flex-direction: column;
 justify-content: space-between;
}
.rollout_right div{
  display: flex;
  flex-direction: column;
 justify-content: center;
}
.rollout_right h5{
  height:px2vw(30);
  font-size:px2vw(30);
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:rgba(38,38,38,1);
  line-height:px2vw(30);
  margin: 0;
}
.rollout_right p{
  height:px2vw(22);
  font-size:px2vw(22);
  font-family:PingFangSC-Light,PingFangSC;
  font-weight:300;
  color:rgba(125,125,125,1);
  line-height:px2vw(22);
  margin-top: px2vw(20);
}
.notice img{
  height: px2vw(32);
  width: px2vw(32);
}
