*{
  font-family: PingFangSC;
}
.group {
  margin-top: px2vw(20);
}
/deep/ .van-field__control {
  height: 100%;
}
/deep/ .van-cell {
  height: px2vw(110);
  padding: 0 px2vw(40);
}
/deep/ .van-field__label {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
/deep/ .van-cell__value {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
/deep/ .van-field__body {
  flex-direction: row;
  justify-content: space-between;
}
.line {
  position: relative;
}
.line::after {
  content: " ";
  height: px2vw(1);
  position: absolute;
  top: -px2vw(0.5);
  left: px2vw(40);
  right: px2vw(40);
  background-color: #EBEEF0;
  z-index: 100;
}
.lineTop {
  position: relative;
}
.lineTop::before {
  content: " ";
  height: px2vw(1);
  position: absolute;
  top: -px2vw(0.5);
  left: px2vw(40);
  right: px2vw(40);
  background-color: #EBEEF0;
  z-index: 100;
}
/deep/ .van-hairline--top-bottom::after {
  border-width: 0;
}
//修改vant的右边图标
/deep/ .van-icon {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  align-self: center;
}
/deep/ .van-icon-arrow:before {
  content: "";
  width: px2vw(12);
  height: px2vw(22);
  background: url("~@/assets/electronicAccounts/icon_home_more.png");
  background-size: 100% 100%;
}
