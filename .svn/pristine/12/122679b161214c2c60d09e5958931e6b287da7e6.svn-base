

.col-group {
  padding-left: px2vw(14);
  background: #fff;
}
.col-item {
  padding: px2vw(12) 0;
  border-bottom: 1px solid #e6e6e6;
  padding-right: px2vw(14);
  line-height: px2vw(20);
  font-size: px2vw(14);
  min-height: px2vw(20);
}
.col-l {
  width: px2vw(90);
  float: left;
}
.col-m {
  margin-left: px2vw(10);
  display: inline-block;
  text-align: justify;
  width: px2vw(220);
}
.col-r {
  margin-left: px2vw(100);
  text-align: justify;
}
.arrow {
  width: px2vw(10);
  float: right;
}
.col-item-in {
  line-height: px2vw(44);
  border-bottom: 1px solid #e6e6e6;
  padding-right: px2vw(14);
  font-size: px2vw(14);
}
.col-item-in .col-in {
  display: inline-block;
  margin-left: px2vw(10);
}
.col-in .add-in-w {
  width: px2vw(140);
}
.col-item-in .add-in-tips {
  position: absolute;
  right: px2vw(14);
  color: #18baf9;
  font-size: px2vw(12);
}
.noborder-b {
  border-bottom: 0 !important;
}
.cancel-type {
  color: #333;
}
.cancel-no {
  margin-left: px2vw(15);
  font-size: px2vw(12);
}
.cancel-address {
  line-height: px2vw(18);
  font-size: px2vw(12);
}

.outer {
  height: 100%;
  display: flex;
  flex-direction: column;
  // justify-content: space-between;
}

.flex-list{
  height: 100%;
  display: flex;
  flex-direction: column;
}