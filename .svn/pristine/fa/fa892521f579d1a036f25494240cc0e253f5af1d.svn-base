import Vue from "vue";
import Router from "vue-router";

//路由懒加载
const Index = () => import("../pages/index.vue");
Vue.use(Router);

export default new Router({
  routes: [
    {
      path: "",
      redirect: "/index"
    },
    {
      path: "/index",
      name: "index",
      component: Index,
      meta: {
        title: "网点地图",
        keepAlive: false
      }
    }
  ]
});
