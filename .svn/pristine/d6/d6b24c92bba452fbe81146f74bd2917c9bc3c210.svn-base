.top {
  margin-top: px2vw(20);
}
.white {
  background: #fff;
}
.left {
  font-size: px2vw(30);
  font-weight: 400;
  color: #262626;
}
.mainContent {
  flex: 1;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}
.mainContent::-webkit-scrollbar {
  display: none; /*隐藏滚动条*/
}
.content {
  height: px2vw(110);
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  .right {
    flex: 1;
    margin-left: px2vw(40);
    font-size: px2vw(24);
    font-weight: 300;
    color: #a8a8a8;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    .iconfont {
      font-size: px2vw(24);
      font-weight: 400;
      margin-top: px2vw(6);
      color: #7d7d7d;
    }
    .selectEnd {
      font-size: px2vw(30);
      font-weight: 400;
      color: #262626;
    }
  }
}
.bottom {
  display: flex;
  flex-direction: row;
  overflow: hidden;
  overflow-x: scroll;
  p {
    width: px2vw(130);
    height: px2vw(56);
    min-width: px2vw(130);
    max-width: px2vw(130);
    line-height: px2vw(56);
    text-align: center;
    box-sizing: border-box;
    border: px2vw(1) solid #7d7d7d;
    color: #7d7d7d;
    border-radius: px2vw(6);
    flex: 1;
    font-size: px2vw(24);
  }
  .active {
    border-color: #3e8fff;
    color: #3e8fff;
  }

  .item:not(:first-child) {
    margin-left: px2vw(30);
  }
}
.bottom::-webkit-scrollbar {
  display: none;
}
.product {
  font-size: 0;
  padding: px2vw(21) px2vw(40) px2vw(20);
  .name {
    font-size: px2vw(32);
    font-weight: 500;
    color: #262626;
    .iconfont {
      font-size: px2vw(22);
      font-weight: 400;
      color: #999ca0;
    }
  }
  .num {
    font-size: px2vw(26);
    font-weight: 400;
    color: #a8a8a8;
    margin-top: px2vw(11);
  }
}
