.money {
  background: #fff;
  height: px2vw(260);
  padding: 0 px2vw(40);
  .title {
    height: px2vw(80);
    line-height: px2vw(80);
    font-size: px2vw(30);
    font-weight: 400;
    color: #262626;
  }
  .toast {
    height: px2vw(60);
    line-height: px2vw(60);
    color: #7d7d7d;
    font-size: px2vw(24);
  }
}
.period {
  height: px2vw(110);
  background: #fff;
  padding: 0 px2vw(40);
  font-size: 0;
  span {
    font-size: px2vw(24);
    font-family: PingFangSC-Light, PingFang SC;
    font-weight: 300;
    color: rgba(125, 125, 125, 1);
  }
  .num {
    color: rgba(62, 143, 255, 1);
  }
}
.methods {
  height: px2vw(110);
  background: #fff;
  padding: 0 px2vw(40);
  .right {
    font-size: px2vw(30);
  }
}
.agree {
  display: flex;
  flex-direction: row;
  padding: 0 px2vw(40);
  padding-bottom: px2vw(92);
  margin-top: px2vw(30);
  .check {
    width: px2vw(24);
    height: px2vw(24);
    background: url("~@/assets/fundCollection/icon_gouxuan_nor@2x.png") no-repeat;
    background-size: 100% 100%;

    .iconfont {
      color: #008bff;
      font-size: px2vw(24);
    }
  }
  .text {
    margin-left: px2vw(10);
    flex: 1;
    font-size: 0;
    line-height: px2vw(30);
    font-weight: 300;
    color: #7d7d7d;
    span {
      font-size: px2vw(22);
    }
    .agreement {
      color: #008bff;
    }
  }
}
.act {
  height: px2vw(196);
}
.referrer {
  height: px2vw(110);
  background: #fff;
  padding: 0 px2vw(40);
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  font-size: px2vw(30);
  .right {
    flex: 1;
    margin-left: px2vw(40);
  }

  //   background: linear-gradient(#ffdfc7, #d69f81);
}
