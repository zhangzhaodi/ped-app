import Vue from "vue";
import Router from "vue-router";

//路由懒加载
const Index = () => import("../pages/index.vue");
const Detail = () => import("../pages/detail.vue");
Vue.use(Router);

export default new Router({
  routes: [
    {
      path: "",
      redirect: "/index"
    },
    {
      path: "/index",
      name: "index",
      component: Index,
      meta: {
        title:'公积金查询'
      }
    },
    {
      path: "/detail",
      name: "detail",
      component: Detail,
      meta: {
        title:'账户详情'
      }
    }
  ]
});
