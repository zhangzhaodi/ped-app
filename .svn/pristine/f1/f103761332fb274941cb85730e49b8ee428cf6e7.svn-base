/* 已签约印章 */
.charge-sign-img {
  position: absolute;
  right: px2vw(20);
  width: px2vw(100);
  top: px2vw(66);
}
/* 金额回显单位 */
.amt-unit {
  position: absolute;
  right: px2vw(14);
}
/* 缴费档次 */
.chargeGrade-box {
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
  flex-wrap: wrap;
  -webkit-align-content: center;
  text-align: center;
  padding: px2vw(9) px2vw(7);
  background: #fff;
  margin-top: px2vw(8);
}
.chargeGrade-box >div {
  margin: px2vw(5) px2vw(7);
  width: px2vw(103);
  text-align: center;
  height: px2vw(28);
  line-height: px2vw(28);
  border-radius: px2vw(5);
  background: #ffffff;
  border: 1px solid #999;
}
.checkstyle{
  background:#0073e5 !important;
  color: #ffffff !important;
  border: 1px solid #0073e5 !important;
}
.uncheckstyle{
  background:#fff !important;
  color: #0073e5 !important;
  border: 1px solid #0073e5 !important;
}
/* 缴费金额自定义输入 */
.amount-box {
  padding: 0 px2vw(14);
  background: #fff;
  padding-top: px2vw(3);
}
.amount-in {
  width: px2vw(300) !important;
  height: px2vw(40);
}
.amount-unit {
  position: absolute;
  right: px2vw(14);
  line-height: px2vw(40);
}
